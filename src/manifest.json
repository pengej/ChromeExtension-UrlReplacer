{
	"manifest_version": 2,
	"name": "UrlReplacer",
	"version": "0.1.0",
	"description": "将一些不能访问的URL地址，替换成同等的可以访问的URL。还可以阻止某些URL的加载。",
	"page_action": {
		"default_icon": {
      "19":"images/icon19.png",
      "38":"images/icon38.png"
		}
	},
	"permissions": [
		"storage",
		"webRequest",
		"webRequestBlocking",
		"*://*/*"
	],
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"options_page": "options.html",
	"author": "PengEJ",
	"homepage_url": "https://github.com/pengej/ChromeExtension-UrlReplacer",
	"update_url": "https://github.com/pengej/ChromeExtension-UrlReplacer/raw/master/update/updates.xml"
}