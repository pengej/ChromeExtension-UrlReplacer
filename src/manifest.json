{
	"manifest_version": 2,
	"name": "UrlReplacer",
	"version": "0.1.1",
	"description": "将一些不能访问的URL地址，替换成同等的可以访问的URL。还可以阻止某些URL的加载。",
	"page_action": {
		"default_icon": {
      "19":"images/icon19.png",
      "38":"images/icon38.png"
		}
	},
	"icons": {
		"16":"images/icon16.png",
		"48":"images/icon48.png",
		"128":"images/icon128.png"
	},
	"permissions": [
		"storage",
		"webRequest",
		"webRequestBlocking",
		"*://*/*"
	],
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"options_page": "options.html",
	"author": "PengEJ",
	"homepage_url": "https://github.com/pengej/ChromeExtension-UrlReplacer"
}